{
    "name": "openapi-express-codegen",
    "version": "1.0.8",
    "description": "Library that generates express Typescript types based on the OpenAPI specification.",
    "author": "Rui Araújo",
    "homepage": "https://github.com/ruiaraujo012/openapi-express-codegen",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ruiaraujo012/openapi-express-codegen.git"
    },
    "bugs": {
        "url": "https://github.com/ruiaraujo012/openapi-express-codegen/issues"
    },
    "license": "MIT",
    "keywords": [
        "openapi",
        "swagger",
        "generator",
        "typescript",
        "yaml",
        "json",
        "express",
        "node"
    ],
    "maintainers": [
        {
            "name": "Rui Araújo"
        }
    ],
    "main": "build/index.js",
    "types": "types/index.d.ts",
    "bin": {
        "express-gen": "bin/index.js"
    },
    "files": [
        "bin/index.js",
        "build/index.js",
        "types/index.d.ts"
    ],
    "scripts": {
        "build": "rollup --config --environment NODE_ENV:development",
        "build:watch": "rollup --config --environment NODE_ENV:development --watch",
        "prerelease": "rimraf ./build ./node_modules/.cache",
        "release": "rollup --config --environment NODE_ENV:production",
        "validate": "tsc --project tsconfig.json --noEmit",
        "eslint": "eslint .",
        "eslint:fix": "eslint . --fix",
        "semantic-release": "semantic-release"
    },
    "dependencies": {
        "camelcase": "6.3.0",
        "commander": "^9.3.0",
        "fs-extra": "^10.1.0",
        "handlebars": "^4.7.7",
        "json-schema-ref-parser": "^9.0.9"
    },
    "devDependencies": {
        "@babel/cli": "7.21.0",
        "@babel/core": "7.21.3",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-typescript": "7.21.0",
        "@rollup/plugin-commonjs": "22.0.2",
        "@rollup/plugin-node-resolve": "13.3.0",
        "@rollup/plugin-typescript": "8.5.0",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/commit-analyzer": "9.0.2",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "8.0.7",
        "@semantic-release/npm": "9.0.2",
        "@semantic-release/release-notes-generator": "10.0.3",
        "@types/cross-spawn": "6.0.2",
        "@types/express": "4.17.17",
        "@types/fs-extra": "9.0.13",
        "@types/glob": "8.1.0",
        "@types/node": "18.15.7",
        "@types/qs": "6.9.7",
        "@typescript-eslint/eslint-plugin": "5.56.0",
        "@typescript-eslint/parser": "5.56.0",
        "abort-controller": "3.0.0",
        "codecov": "3.8.3",
        "cross-spawn": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.36.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
        "express": "4.18.2",
        "form-data": "4.0.0",
        "glob": "8.1.0",
        "prettier": "2.8.7",
        "puppeteer": "15.5.0",
        "qs": "6.11.1",
        "rimraf": "3.0.2",
        "rollup": "2.79.1",
        "rollup-plugin-terser": "7.0.2",
        "rxjs": "7.8.0",
        "semantic-release": "19.0.5",
        "ts-node": "10.9.1",
        "tslib": "2.5.0",
        "typescript": "5.0.2",
        "zone.js": "0.13.0"
    }
}
